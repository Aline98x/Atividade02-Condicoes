import java.util.Scanner;

public class parouimpar {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int numero = 0;

        System.out.println("Digite um número: ");
        numero = sc.nextInt();

        //se -> comparação
        //% -> divisao
        //== -> igual
        if (numero % 2 == 0) {
            System.out.println("Par");
        }
        else {
            System.out.println("Impar");
        }

    }

}
